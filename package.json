{
  "name": "@blockfrost/blockfrost-yoroi-bridge",
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "yarn clean",
    "dev": "ts-node-dev --respawn ./src/server.ts",
    "build": "yarn clean && babel ./src --out-dir dist --copy-files --extensions '.ts'",
    "start": "yarn build && node dist/index.js",
    "lint": "eslint ./src/**/*.ts",
    "type-check": "tsc --project tsconfig.json",
    "test": "yarn jest",
    "test-integration": "BASE_URL=http://127.0.0.1:3007 jest -c test/integration/jest.config.js"
  },
  "dependencies": {
    "@babel/runtime": "^7.14.0",
    "@blockfrost/blockfrost-js": "^0.4.0",
    "@yarnpkg/pnpify": "^2.4.0",
    "bech32": "^2.0.0",
    "bignumber.js": "^9.0.1",
    "cross-env": "^7.0.3",
    "fastify": "^3.15.1",
    "fastify-autoload": "^3.7.1",
    "pino-pretty": "^4.7.1",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-typescript": "^7.13.0",
    "@types/expect-puppeteer": "^4.4.5",
    "@types/jest": "^26.0.22",
    "@types/jest-environment-puppeteer": "^4.4.1",
    "@types/node": "^15.0.1",
    "@types/puppeteer": "^5.4.3",
    "@types/url-join": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "^4.20.0",
    "axios": "^0.21.1",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.23.1",
    "expect-puppeteer": "^5.0.3",
    "jest": "^26.6.3",
    "jest-extended": "^0.11.5",
    "jest-puppeteer": "^5.0.3",
    "prettier": "^2.2.1",
    "puppeteer": "^9.1.1",
    "puppeteer-core": "^9.1.1",
    "ts-jest": "^26.5.4",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.3",
    "url-join": "^4.0.1"
  }
